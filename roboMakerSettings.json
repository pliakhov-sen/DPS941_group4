{
  "runConfigurations": [
    {
      "id": "CloudWatch_Cfg01",
      "name": "CloudWatch Robot",
      "type": "colcon build",
      "cfg": {
        "workingDir": "./CloudWatch/robot_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "CloudWatch_Cfg02",
      "name": "CloudWatch Robot",
      "type": "colcon bundle",
      "cfg": {
        "workingDir": "./CloudWatch/robot_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "CloudWatch_Cfg03",
      "name": "CloudWatch Simulation",
      "type": "colcon build",
      "cfg": {
        "workingDir": "./CloudWatch/simulation_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "CloudWatch_Cfg04",
      "name": "CloudWatch Simulation",
      "type": "colcon bundle",
      "cfg": {
        "workingDir": "./CloudWatch/simulation_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "CloudWatch_SimulationJob",
      "name": "CloudWatch Robot Monitoring",
      "type": "simulation",
      "cfg": {
        "robotApp": {
          "name": "RoboMakerCloudWatchRobotMelodic",
          "sourceBundleFile": "/lab1/aws-robomaker-sample-application-cloudwatch/robot_ws/bundle/output.tar",
          "s3Bucket": "lab1bucketpaul",
          "architecture": "X86_64",
          "launchConfig": {
            "packageName": "cloudwatch_robot",
            "launchFile": "rotate.launch",
            "environmentVariables": {
              "TURTLEBOT3_MODEL": "waffle_pi"
            }
          },
          "robotSoftwareSuite": {
            "name": "ROS",
            "version": "Melodic"
          }
        },
        "simulationApp": {
          "name": "RoboMakerCloudWatchSimulationMelodic",
          "sourceBundleFile": "/lab1/aws-robomaker-sample-application-cloudwatch/simulation_ws/bundle/output.tar",
          "architecture": "X86_64",
          "s3Bucket": "lab1bucketpaul",
          "launchConfig": {
            "packageName": "cloudwatch_simulation",
            "launchFile": "bookstore_turtlebot_navigation.launch",
            "environmentVariables": {
              "TURTLEBOT3_MODEL": "waffle_pi"
            }
          },
          "simulationSoftwareSuite": {
            "name": "Gazebo",
            "version": "9"
          },
          "renderingEngine": {
            "name": "OGRE",
            "version": "1.x"
          },
          "robotSoftwareSuite": {
            "name": "ROS",
            "version": "Melodic"
          }
        },
        "simulation": {
          "maxJobDurationInSeconds": 28800,
          "failureBehavior": "Fail",
          "iamRole": "arn:aws:iam::359110938601:role/robomaker_students",
          "outputLocation": "lab1bucketpaul",
          "tags": {}
        }
      }
    },
    {
      "id": "CloudWatch_wf1",
      "type": "workflow",
      "name": "CloudWatch - Build and Bundle All",
      "runCfgIds": [
        "CloudWatch_Cfg01",
        "CloudWatch_Cfg02",
        "CloudWatch_Cfg03",
        "CloudWatch_Cfg04"
      ]
    }
  ]
}